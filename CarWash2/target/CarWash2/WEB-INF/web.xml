<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>ShowingAllOrders</servlet-name>
		<servlet-class>controller.ShowingAllOrders</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GettingOrdersWithDiscount</servlet-name>
		<servlet-class>controller.GettingOrdersWithDiscount</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ShowingAllOrders</servlet-name>
		<url-pattern>/ShowingAllOrders</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GettingOrdersWithDiscount</servlet-name>
		<url-pattern>/GettingOrdersWithDiscount</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>

